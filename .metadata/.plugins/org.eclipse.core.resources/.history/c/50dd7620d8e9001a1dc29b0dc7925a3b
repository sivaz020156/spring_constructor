package constructor_injection_6;

public class Question {
	private int id;  
	private String name;  
	private Map<Answer,User> answers;  
	  
	public Question() {}  
	public Question(int id, String name, Map<Answer, User> answers) {  
	    super();  
	    this.id = id;  
	    this.name = name;  
	    this.answers = answers;  
	} 
}
