package constructor_injection_4;

public class Question {

	private int id;  
	private String name;  
	private List<Answer> answers;  
	  
	public Question() {}  
	public Question(int id, String name, List<Answer> answers) {  
	    super();  
	    this.id = id;  
	    this.name = name;  
	    this.answers = answers;  
	}  
	
	
}
